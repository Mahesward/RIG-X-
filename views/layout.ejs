  <link rel="stylesheet" href="/stylesheets/demo-4.css">

  <script src="https://www.paypal.com/sdk/js?client-id=<%= locals.paypalClientId %>&currency=USD"></script>

  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
  
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.5.3/jspdf.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf-autotable/3.5.6/jspdf.plugin.autotable.min.js"></script>


</head>



<body>


  <%- include('partials/header-main') %>

  <%- body %>


  <%- include('partials/footer') %>


  <script>
    function toast() {
      Swal.fire({
        toast: true,
        icon: 'success',
        title: 'Item Added To Cart',
        animation: true,
        position: 'bottom',
        showConfirmButton: false,
        timer: 3000,
        timerProgressBar: true,
        didOpen: (toast) => {
          toast.addEventListener('mouseenter', Swal.stopTimer)
          toast.addEventListener('mouseleave', Swal.resumeTimer)
        }
      })
    }


    function toastOutOfStock() {
      Swal.fire({
        toast: true,
        icon: 'error',
        title: 'Product Out Of Stock!',
        animation: true,
        position: 'bottom',
        showConfirmButton: false,
        timer: 3000,
        timerProgressBar: true,
        didOpen: (toast) => {
          toast.addEventListener('mouseenter', Swal.stopTimer)
          toast.addEventListener('mouseleave', Swal.resumeTimer)
        }
      })
    }
  </script>


  <!-- Plugins JS File -->
  <script src="/javascripts/jquery.min.js"></script>
  <script src="/javascripts/bootstrap.bundle.min.js"></script>
  <script src="/javascripts/jquery.hoverIntent.min.js"></script>
  <script src="/javascripts/jquery.waypoints.min.js"></script>
